package boot.study.Controller;

import java.util.ArrayList;
import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PostMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.ResponseBody;
import org.springframework.web.multipart.MultipartFile;


import naver.cloud.NcpObjectStorageService;



import boot.study.service.LoginUserService;
import naver.cloud.NcpObjectStorageService;

@Controller
@RequestMapping("/login")
public class LoginController {

		
	  @Autowired
	    private LoginUserService loginuserService;
	  
	  @Autowired
		private NcpObjectStorageService storageService;
	  
	//버켓이름지정
		private String bucketName="jobcamp-bucket";

	    @GetMapping("/mainlogin")
	    public String mainloginPage() {
	        return "/main/login/main_login";
	    }


	    @GetMapping("/NewFile")
	    public String personalLoginPage() {
	    	 return "/login/NewFile";
	    }
	    
	   

	    @GetMapping("/company/login")
	    public String companyLoginPage() {
	        return "/cl_login";
	    }
	    
	    @GetMapping("/logout")
	    public String logoutPage() {
	    	return "/logout";
	    }	
	
	
	
}
